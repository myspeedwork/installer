{
  "name": "speedwork/installer",
  "type": "composer-plugin",
  "description": "A composer plugin, to help install packages of different types in custom paths.",
  "license": "MIT",
  "keywords": ["composer", "composer-plugin", "composer-installer"],
  "authors": [
    {
      "name": "Sankar suda",
      "email": "sankar.suda@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "composer-plugin-api": "*"
  },
  "require-dev" : {
        "phpunit/phpunit": "~3.7",
        "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {"Speedwork\\Installer\\": "src/"}
  },
  "minimum-stability": "dev",
  "preferred-install": "dist",
  "extra": {
      "class": ["Speedwork\\Installer\\LibraryPlugin"],
      "branch-alias": {
        "dev-master": "1.0.x-dev"
      }
  }
}
