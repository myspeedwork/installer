{
    "name": "speedwork/installer",
    "type": "composer-plugin",
    "description": "A composer plugin, to help install packages of different types.",
    "license": "MIT",
    "authors": [{
        "name": "Sankar suda",
        "email": "sankar.suda@gmail.com"
    }],
    "require": {
        "php": ">=5.6",
        "composer-plugin-api": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "satooshi/php-coveralls": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Speedwork\\Installer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "class": ["Speedwork\\Installer\\LibraryPlugin"],
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
